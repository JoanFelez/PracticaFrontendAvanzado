<div class="login-form inactive" id="login-form">
        <form class="login">
            <header id="login-header" class="login-header">
                <p id="form-title" class="title">Log in</p>
                <button id="close-button" class="close-button"><i class="fas fa-times close-icon""></i></button>
            </header>
            <input id="user-input" type="text" placeholder="Username" autofocus/>
            <i id="user-icon" class="fa fa-user"></i>
            <input id="pass-input" type="password" placeholder="Password" />
            <i id="pass-icon" class="fa fa-key"></i>
            <a id="form-link" href="#">Forgot your password?</a>
            <button id="state-button" class="state-button">
                <i id="state-spinner" class="spinner"></i>
                <span id="state-span" class="state">Log in</span>
            </button>
        </form>
    </div>